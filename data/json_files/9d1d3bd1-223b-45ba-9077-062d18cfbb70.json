    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 27879,
      "end_time_ms": 33049,
      "num_slots": 1,
      "text": "Apə mapwe, əgɨndə Lisa ko ətə kwapwe.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Yumi stap long hia long dispela hap nau Lisa istap."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 33049,
      "end_time_ms": 35329,
      "num_slots": 1,
      "text": "Əpəkig urusag biog bo marupecog.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Tupela haus bilong mipela em yumi brukim pinis."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 36229,
      "end_time_ms": 45769,
      "num_slots": 1,
      "text": "Mapwe nau, Timothy ino— iolsem ən— em nɨmbo narahaɲ, tasol ənən neyagureh, tasol ino niyagureh gut.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Mipela stap nau, Timothy ino - iolsem - em wokabaut finis, em itoktok tasol, tasol em ino toktok gut."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 46019,
      "end_time_ms": 46979,
      "num_slots": 1,
      "text": "Na mapwe.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Na mipela istap."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 47629,
      "end_time_ms": 56589,
      "num_slots": 1,
      "text": "Mapwe nau, yaken— Leopold yaken nani aməkeik cabʉh yous, catowi, cati əpə mapwe.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Mipela istap nau – papa bilong Leopold wantaim mama bilong Leopold ol igo daun long nambis, nau ol ikam antap, nau ol ilukim mipela istap."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 57539,
      "end_time_ms": 59549,
      "num_slots": 1,
      "text": "Mapwe nau, cenaməp capwe.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Mipela istap nau ol ikam nau ol istap wantaim mipela."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 59845,
      "end_time_ms": 67735,
      "num_slots": 1,
      "text": "Capwe nau, yaken— Leopold yaken nakəna nakərip yaken əpəkin nakana, \"Ei yakərieɲ eɲɨndə waia.\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Ol istap nau, papa bilong Leopold em itok em itokim papa bilong mipela em itok, \"Mi laikim dispela waia.\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 68035,
      "end_time_ms": 70505,
      "num_slots": 1,
      "text": "\"Ɲakɨs gani arere long ipəkig dewɔg\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "\"Em istap clostu long arare bilong toilet bilong yupela.\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 70826,
      "end_time_ms": 71916,
      "num_slots": 1,
      "text": "Na ia.",
      "dependents": []
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 72896,
      "end_time_ms": 79196,
      "num_slots": 1,
      "text": "Papa bilong ol nakəna, \"Wak. Ino əpəkiɲ waia, ececiɲ waia. Ignas acəkə ececiɲ.\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Papa bilong ol em itok, \"Nogat. Ino waia bilong mipela, waia bilong ol. Bilong lain bilong Ignas.\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 79514,
      "end_time_ms": 84664,
      "num_slots": 1,
      "text": "\"Bihain cinaki ɲɨkəripec, orait ɲaka ɲɨɲɨhɨr orait ɲɨna ɲɨɲɨhɨr. Əpə wa ececiɲ.\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "\"Bihain ol ikam orait yu tokim ol, orait sapos ol laikim long yu kisim orait yu ken kisim. Em ino bilong mipela, bilong ol.\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 85324,
      "end_time_ms": 87974,
      "num_slots": 1,
      "text": "Əriə nakana, \"Orait ina bo ina icekimweɲ.\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Na em itok, \"Mi go na checkim.\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 88294,
      "end_time_ms": 89144,
      "num_slots": 1,
      "text": "Əriə nəna.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Olsem na em igo."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 89590,
      "end_time_ms": 96240,
      "num_slots": 1,
      "text": "Nəna narih nəna ga dewɔg, na Timothy ənən nakita nagipwecən, nənak apə ino mudekemecum ənən nəna.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Em igo na em igo sait long toilet na Timothy em ikirap bihainim em, igo mipela ino save na em igo."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 97260,
      "end_time_ms": 103190,
      "num_slots": 1,
      "text": "Bihain ənən nənak, ənən nəna narih gani wɔbʉr— aromom hənamogi dewɔg tokwɔnig.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Bihain em igo, na em igo raunim narapela sait – long toilet long sait bilong man."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 103569,
      "end_time_ms": 111609,
      "num_slots": 1,
      "text": "Orait Timothy nəna əgɨdə əpə arəmagou əpəkig tokwɔnig nəna, ənən nəna nawic etiɲ, gi nəna nabʉh dewɔg.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Orait Timothy em iran igo long sait bilong ol meri isave yusim longen hap na igo insait, nau em igo na em igodaun long toilet."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 112245,
      "end_time_ms": 120175,
      "num_slots": 1,
      "text": "Ati ino olsem— ənən nəkana nɨgɨrɨkɨk dewɔg, nati olsem spirit nongut o, catɨkən nabʉh əgɨdə dewɔg.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Ating em ino pundaun tru tu long toilet ol spirit nogut iputim em igo daun long dispela toilet."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 121175,
      "end_time_ms": 123383,
      "num_slots": 1,
      "text": "Nabʉh nau, nəminek.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Em igo na em iharim."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 124002,
      "end_time_ms": 126812,
      "num_slots": 1,
      "text": "Yaken— Leopold yaken napwe nanɨk waia.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "[Na em iharim] papa bilong Leopold papa wok long pulim waia."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 126856,
      "end_time_ms": 129646,
      "num_slots": 1,
      "text": "Na nəmin ənən olsem nəreh.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Na em iharim olsem em icrai."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 129816,
      "end_time_ms": 133886,
      "num_slots": 1,
      "text": "Na gi nabʉh olgeta nəne pas əgɨndə ereh bieh.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Na em igo daun olgeta long hol na pas bilong tupela solda bilong em istap."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 134196,
      "end_time_ms": 138936,
      "num_slots": 1,
      "text": "Na nabʉh na aias gi sabʉh iah ənən nəne pas ereh na napw— stap.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Na em igo daun olgeta lek bilong em igo daun go daun olgeta na em pas long tupela solda na em istap."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 139537,
      "end_time_ms": 143377,
      "num_slots": 1,
      "text": "Əriə yaken Leopold yaken neminek nereh nau nakana, \"Eih!\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Olsem papa bilong Leopold em iharim em icrai nau em itok, \"Eih!\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 144337,
      "end_time_ms": 146927,
      "num_slots": 1,
      "text": "\"Əneɲ batowiɲ mas ɲereh dewɔg ia!\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "\"Wanpela pikinini mas crai long toilet ia!\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 147637,
      "end_time_ms": 154267,
      "num_slots": 1,
      "text": "Əriə nəne raun nənaki, naka nɨti, nati ənən— gi nabʉh iəh ənən nəne pas agɨndə ereh bieh.",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Olsem na em iraun ikam, na em ilukim, na em ilukim em igo daun olgeta pinis na em ipas long ol solda, tupela solda."
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 154807,
      "end_time_ms": 157377,
      "num_slots": 1,
      "text": "Əriə ei yeteponɨkɨk na yaka, \"Eih!\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "Nau mi go luk fainim na mi tok, \"Eih!\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 158027,
      "end_time_ms": 159887,
      "num_slots": 1,
      "text": "\"Timothy gi nənak, o?\"",
      "dependents": [
        {
          "tier": "T3",
          "values": [
            {
              "start_slot": 0,
              "end_slot": 1,
              "value": "\"Timothy go we, o?\""
            }
          ]
        }
      ]
    },
    {
      "speaker": "S2",
      "tier": "T2",
      "start_time_ms": 160717,
      "end_time_ms": 164527,
      "num_slots": 1,
      "text": "Na bihain na, yaken Leopold yaken nohʷarəpi naka, \"Owak.\"",
      "dependents": [
        {
